{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "<rootDir>/test/prisma-test-environment.ts",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!nanoid)"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/test/setup-file.ts"
  ],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}